{
  "comments": {
    "lineComment": "//",
    "blockComment": [ "/*", "*/" ]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"]
  ],
  "autoClosingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"],
    { "open": "/**", "close": " */", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["\"", "\""],
    ["'", "'"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*?(\\.(define|disk|enum|for|function|if|macro|modify|namespace|pseudocommand|pseudopc|while|zp|assert).*?\\{|#if|!?[a-zA-Z0-9_]+:\\s*?\\{|\\{|\\}\\s*else.*?\\{)",
      "end": "^\\s*?(\\}|#endif)"
    }
  }
}